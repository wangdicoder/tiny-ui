{
  "name": "tiny-ui",
  "version": "0.0.94",
  "description": "A friendly ui component set for react",
  "license": "MIT",
  "keywords": [
    "tiny",
    "tiny-ui",
    "design",
    "react",
    "react-component",
    "component",
    "components",
    "ui",
    "frontend",
    "react hook component",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wangdicoder/tiny-ui.git"
  },
  "author": "Di Wang<wangdicoder@gmail.com>",
  "homepage": "https://tiny-ui.dev/",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "npm run build-ts && npm run dist",
    "commit": "git add . && ./node_modules/cz-customizable/standalone.js",
    "build-ts": "rimraf lib && tsc -p tsconfig.json",
    "dist-style": "gulp --gulpfile scripts/gulpfile.js && node scripts/generate-style.js",
    "dist-dev": "cross-env NODE_ENV=development webpack --config webpack.config.js --progress --colors",
    "dist-prod": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress --colors",
    "dist": "rimraf -rf dist && npm run dist-dev && npm run dist-prod && npm run dist-style",
    "dev": "cross-env NODE_ENV=development webpack serve --config site/config/webpack.dev.js",
    "eslint": "eslint components --ext .ts,.tsx",
    "release": "standard-version",
    "site:build": "cross-env NODE_ENV=production webpack --config site/config/webpack.prod.js",
    "site:deploy": "cross-env NODE_ENV=production now",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "lint:style": "stylelint components/**/*.scss --syntax scss",
    "lint:style:fix": "npm run lint:style -- --fix"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "dependencies": {
    "core-js": "^3.9.0",
    "@popperjs/core": "^2.11.4",
    "classnames": "^2.3.1",
    "intersection-observer": "^0.12.0",
    "raf": "^3.4.1",
    "react-transition-group": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.2.3",
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/raf": "^3.4.0",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.4",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.3",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "clean-webpack-plugin": "^4.0.0",
    "commitlint-config-cz": "^0.13.3",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.21.0",
    "eslint-plugin-jest": "^24.2.1",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss-loader": "^6.2.1",
    "postcss-nested": "^5.0.6",
    "prettier": "2.6.0",
    "prism-react-renderer": "^1.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-live": "^2.4.1",
    "react-refresh": "^0.11.0",
    "react-router-dom": "^5.3.0",
    "remark-gfm": "^3.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "standard-version": "^9.3.2",
    "style-loader": "^3.3.1",
    "stylelint": "^14.6.0",
    "stylelint-config-standard": "^25.0.0",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^27.1.3",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  },
  "lint-staged": {
    "components/**/*.scss": [
      "npm run lint:style:fix"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "Firefox ESR",
    "IE >= 11",
    "Edge >= 14"
  ]
}
