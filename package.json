{
  "name": "tiny-ui",
  "version": "0.0.73",
  "description": "A friendly ui component set for react",
  "license": "MIT",
  "keywords": [
    "tiny",
    "tiny-ui",
    "design",
    "react",
    "react-component",
    "component",
    "components",
    "ui",
    "frontend",
    "react hook component",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wangdicoder/tiny-ui.git"
  },
  "author": "Di Wang<wangdicoder@gmail.com>",
  "homepage": "https://tiny-ui.dev/",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf lib && tsc -p tsconfig.json && npm run dist",
    "commit": "git add . && ./node_modules/cz-customizable/standalone.js",
    "dist-style": "gulp --gulpfile scripts/gulpfile.js && node scripts/generate-style.js",
    "dist-dev": "NODE_ENV=development webpack --config webpack.config.js --progress --colors",
    "dist-prod": "NODE_ENV=production webpack --config webpack.config.js --progress --colors",
    "dist": "rm -rf dist && npm run dist-dev && npm run dist-prod && npm run dist-style",
    "dev": "webpack-dev-server --config site/config/webpack.dev.js",
    "eslint": "eslint components --ext .ts,.tsx",
    "release": "standard-version",
    "site:build": "webpack --config site/config/webpack.prod.js",
    "site:deploy": "npm run site:build && gh-pages -d site/build",
    "test": "jest"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "raf": "^3.4.1",
    "react-transition-group": "^4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@commitlint/cli": "^8.2.0",
    "@mdx-js/loader": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.146",
    "@types/node": "^12.12.7",
    "@types/raf": "^3.4.0",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@types/react-router-dom": "^5.1.0",
    "@types/react-transition-group": "^4.2.3",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.6.1",
    "autoprefixer": "^9.6.5",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "commitlint-config-cz": "^0.12.1",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^3.2.0",
    "cz-customizable": "^6.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.5.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "file-loader": "^4.2.0",
    "gh-pages": "^2.1.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "prettier": "1.19.1",
    "prism-react-renderer": "^1.0.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-live": "^2.2.1",
